<form method="post" id="form_delete" action="{{ path('app_salida_delete', {'id': salida.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ salida.id) }}">
    <button class="btn">Borrar</button>
</form>
