{% extends 'base.html.twig' %}



{% block body %}
    <div class="container mt-4">
        <h2>Datos</h2>
        <div class="row">
            {% for key, value in data %}
                <div class="col-md-4 mb-3">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">{{ key }}</h5>
                            

                        </div>
                    </div>
                </div>
                {% if loop.index % 3 == 0 %}
                    </div><div class="row">
                {% endif %}
            {% endfor %}
        </div>
    </div>
{% endblock %}
